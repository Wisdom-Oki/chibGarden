# Declare member variables here. Examples:
var names = \"Him\"
var hunger = 100
var stamina = 50
var strength = 55
var love = 30

# Called when the node enters the scene tree for the first time.
func _ready():
	get_node(\"Sprite/hunger\").set_value(hunger)
	get_node(\"Sprite/stamina\").set_value(stamina)
	get_node(\"Sprite/strength\").set_value(strength)
	get_node(\"Sprite/love\").set_value(love)

	
	var button = Button.new()
	button.text = \"add\"
	button.connect(\"pressed\", self, \"_button_pressed\")
	add_child(button)
func _button_pressed():
	stamina = stamina +10
	get_node(\"Sprite/stamina\").set_value(stamina)



