# Declare member variables here. Examples:
var names = \"God of chib\"
var hunger = 100
var stamina = 50
var strength = 55
var Happniess = 30
var age
var eyes
var Larm
var Lleg
var Rarm
var Rleg
var rarity
var safearea

# Called when the node enters the scene tree for the first time.
func _ready():
	safearea = OS.get_window_safe_area()
	print(safearea)
	
	get_node(\"hunger\").set_value(hunger)
	get_node(\"stamina\").set_value(stamina)
	get_node(\"strength\").set_value(strength)
	get_node(\"happiness\").set_value(Happniess)

	
	var button = Button.new()
	button.text = \"add\"
	button.connect(\"pressed\", self, \"_button_pressed\")
	add_child(button)
func _button_pressed():
	stamina = stamina +10
	get_node(\"stamina\").set_value(stamina)



